{
  "name": "git-fancy-status",
  "version": "1.0.8",
  "description": "Get more information about your git repository.",
  "main": "bundle.js",
  "scripts": {
    "bundle": "node ./src/bundle.js",
    "prepublish": "npm run bundle",
    "dev": "nodemon ./src/bundle.js"
  },
  "preferGlobal": true,
  "bin": {
    "gs": "./bin/git-status",
    "git-fancy-status": "./bin/git-status",
    "gc": "./bin/git-commit",
    "git-fancy-commit": "./bin/git-commit"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/zhabinsky/git-fancy-status.git"
  },
  "keywords": [
    "git",
    "status",
    "bash",
    "cli"
  ],
  "author": "Vladislav Zhabinsky <vladislavzhabinsky@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zhabinsky/git-fancy-status/issues"
  },
  "homepage": "https://github.com/zhabinsky/git-fancy-status#readme",
  "devDependencies": {
    "bash-minifier": "^0.1.3"
  },
  "dependencies": {
    "i": "^0.3.6",
    "npm": "^6.14.8",
    "shelljs": "^0.8.4",
    "uglify-js": "^3.11.4"
  }
}
